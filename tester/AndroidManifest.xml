<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emediate.android.emediatesdktester"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WACCESS_COARSE_LOCATION" />
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.emediate.android.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.emediate.android.AdsCreateActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name="com.emediate.android.AdsSampleActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name="com.emediate.android.AdsSampleMapActivity"
            android:label="@string/title_activity_main" />
        
        <activity android:name="com.emediate.view.Browser" />
        <activity android:name="com.emediate.view.MraidActionHandler" />

        <uses-library android:name="com.google.android.maps" />
    </application>


</manifest>